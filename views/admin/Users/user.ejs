<%- include('../../partials/header.ejs') %>
<%- include('../../partials/navbar.ejs') %>
<%- include('../../partials/topo1.ejs') %>



<div class="container">
	<hr>
	<h2>Usuarios</h2>
	<a href="/admin/users/create" class="btn btn-success btn-sm">NOVOS USUARIO</a>
	<hr>
	<table class="table table-bordered">
		<thead>
			<tr>
			   <th>ID</th>
			   <th>E-mail</th>
			   
			</tr>
		</thead>
		<tbody>
			
			<% users.forEach(user =>{ %>
                <tr>
			   <th><%= user.id %></th>
			   <th><%= user.email %></th>
			   
			   
			</tr>
		    <% }) %>
		</tbody>
	</table>
</div>


